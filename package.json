{
  "private": true,
  "name": "remix-app-template",
  "description": "",
  "license": "",
  "scripts": {
    "postinstall": "patch-package && remix setup cloudflare-workers",
    "build": "npm run build:css && remix build",
    "build:css": "postcss styles/global.css -o ./app/styles/global.css",
    "dev": "concurrently -c \"#3defe9\",\"#f44250\",\"#fecc1b\" -n css,remix,miniflare \"npm run dev:css\" \"remix watch\"  \"npm run start\"",
    "dev:css": "postcss styles/global.css -o ./app/styles/global.css --watch",
    "build:worker": "esbuild --define:process.env.NODE_ENV='\"production\"' --minify --bundle --sourcemap --outdir=dist ./worker",
    "dev:worker": "esbuild --define:process.env.NODE_ENV='\"development\"' --bundle --sourcemap --outdir=dist ./worker",
    "start": "miniflare --build-command \"npm run dev:worker\" --watch",
    "clean": "rimraf dist build public/build .cache .mf",
    "deploy": "npm run clean && npm run build && wrangler publish"
  },
  "dependencies": {
    "@prisma/client": "^3.6.0",
    "@remix-run/cloudflare-workers": "0.0.0-experimental-ab9dac4f",
    "@remix-run/react": "0.0.0-experimental-ab9dac4f",
    "clsx": "^1.1.1",
    "prisma": "^3.6.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "remix": "0.0.0-experimental-ab9dac4f",
    "remix-auth": "^2.5.0-0",
    "remix-validated-form": "^1.1.1",
    "url-pattern": "^1.0.3",
    "uuid": "^8.3.2",
    "yup": "^0.32.11"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^2.2.2",
    "@esbuild-plugins/node-modules-polyfill": "^0.1.2",
    "@remix-run/dev": "0.0.0-experimental-ab9dac4f",
    "@types/react": "^17.0.24",
    "@types/react-dom": "^17.0.9",
    "@types/uuid": "^8.3.3",
    "autoprefixer": "^10.4.0",
    "concurrently": "^6.4.0",
    "cssnano": "^5.0.12",
    "esbuild": "0.13.14",
    "esbuild-plugin-alias": "^0.2.1",
    "miniflare": "2.0.0-next.3",
    "patch-package": "^6.4.7",
    "postcss": "^8.4.4",
    "postcss-cli": "^9.0.2",
    "rimraf": "^3.0.2",
    "tailwindcss": "^2.2.19",
    "typescript": "^4.1.2"
  },
  "engines": {
    "node": ">=14"
  },
  "sideEffects": false,
  "main": "dist/worker.js"
}
